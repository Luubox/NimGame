<Window x:Class="NimGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NimGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition Height="75" />
            <RowDefinition Height="75" />
            <RowDefinition Height="75" />
            <RowDefinition Height="75" />
            <RowDefinition Height="75" />
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
        </Grid.ColumnDefinitions>

        <Button Name="Row1Button" 
                Grid.Column="7" Grid.Row="0" Content="Row 1" 
                Margin="10,10,10,10"/>
        <Button Name="Row2Button" 
                Grid.Column="7" Grid.Row="1" Content="Row 2"
                Margin="10,10,10,10"/>
        <Button Name="Row3Button"  
                Grid.Column="7" Grid.Row="2" Content="Row 3"
                Margin="10,10,10,10"/>
        <Button Name="Row4Button" 
                Grid.Column="7" Grid.Row="3" Content="Row 4"
                Margin="10,10,10,10"/>
        <Button Name="NextTurnButton" 
                Grid.Column="7" Grid.Row="4" Content="Next Turn"
                Margin="10,10,10,10"/>

